.container{
    background-color:white;
}
/* 浮动的优惠券->领券中心 */
.levitate{
    width:100rpx;
    height:230rpx;
    position:fixed;
    bottom:25rpx;
    right:25rpx;
    display:flex;
    flex-direction:column;
    justify-content:space-around;
    align-items:center;
    margin-right: 20rpx;
  }
.levitateview{
    width:100rpx;
    height:100rpx;
    display:flex;
    overflow:hidden;
    border-radius:5rpx;
    justify-content:space-around;
    align-items:center;
  }
.levitater{
    background:#f23030;
  }
  /* 立刻领券-top */
  .coupon-top{
    display: inline-block;
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    z-index: 10;
    background-color: #282828;
    color: white;
    line-height: 100rpx;
  }
  .coupon-top image{
    text-align: center;
    width: 80rpx;
    height: 80rpx;

  }
  .coupon-top-q{
    display: inline-block;
    text-align: center;
    width: 400rpx;
  }
  .coupon-top-l{
    width: 200rpx;
    float: right;
    border-radius: 10rpx;
    text-align: center;
    background-color: #00CED1;
  }
  /* 搜索栏  */
  .search-box{
    display: flex;
    margin-top: 8rpx;
    margin-bottom: 8rpx;
    width: 100%;
    height: 88rpx;
    z-index: 10;
    background: linear-gradient(rgba(0,0,0,.2),rgba(255,255,255,0)); 
}
.search-lift{
    padding-top: 10rpx;
    width:10%;
}
.search-lift-image{
    width: 70rpx;
    height: 80rpx;
    margin-right: 10rpx;
    padding-left: 10rpx;
}
.logo{
    width:48rpx;
    height:48rpx;
    margin:0 18rpx;
    border-radius: 50%;
    overflow: hidden;
}
.search-inner{
    display: flex;
    align-items: center;
    left: 60rxp;
    margin:15rpx 10rpx;
    width:75%;
    height: 58rpx;
    border-radius:8rpx;
    background-color: #fff;
}
.search-img{
    margin-right:12rpx;
    width: 30rpx;
    height: 30rpx;
    margin-top: -6rpx;
    padding-left: 15rpx;
}
.search-cont{
    height: 58rpx;
    width:536rpx;
    padding-left: 12rpx;
    line-height: 58rpx;
    font-size: 24rpx;
    color: #666;
}
.search-right{
    /* justify-content: space-between;
    align-items: center; */
    padding-top: 0rpx;
    padding-left: 10rpx;
    width:10%;
}
.search-right-image{
    width: 70rpx;
    height: 80rpx;
    color: white;
}
.search-fixed{
    background:rgba(242,48,48,0.8);
}
/* 轮播图 */
swiper {
    width: 100%;
   /* height: calc(100vw * 240 / 414);*/
   height: 800rpx;
}
.slide-image{
    width: 100%;
    height: 800rpx;
}
